{
    "name": "vis-stilling",
    "version": "0.1.0",
    "private": true,
    "homepage": "/arbeid/stilling",
    "dependencies": {
        "@craco/craco": "^6.4.3",
        "@navikt/ds-css": "^0.16.13",
        "@navikt/ds-icons": "^0.8.6",
        "@navikt/ds-react": "^0.17.16",
        "@types/node": "^17.0.23",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.14",
        "amplitude-js": "^8.17.0",
        "classnames": "^2.3.1",
        "craco-less": "^2.0.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-scripts": "^5.0.0",
        "typescript": "^4.6.3"
    },
    "devDependencies": {
        "@types/amplitude-js": "^8.16.0",
        "fetch-mock": "^9.11.0",
        "husky": "^7.0.0",
        "prettier": "^2.6.1",
        "pretty-quick": "^3.1.3"
    },
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "eject": "craco eject",
        "format": "prettier --write .",
        "start:mock": "REACT_APP_MOCK=true npm start",
        "server:install": "cd server && npm ci",
        "server:build": "tsc -b server/tsconfig.json",
        "server:start": "node server/build/server.js",
        "prepare": "husky install"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5",
        "arrowParens": "always",
        "printWidth": 100
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
