{
    "name": "vis-stilling",
    "version": "0.1.0",
    "private": true,
    "homepage": "/arbeid/stilling",
    "dependencies": {
        "@navikt/ds-css": "^1.3.13",
        "@navikt/ds-icons": "^1.3.13",
        "@navikt/ds-react": "^1.3.13",
        "amplitude-js": "^8.21.1",
        "classnames": "^2.3.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^4.8.4"
    },
    "devDependencies": {
        "@types/amplitude-js": "^8.16.2",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "fetch-mock": "^9.11.0",
        "husky": "^8.0.1",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "react-scripts": "^5.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "format": "prettier --write .",
        "start:mock": "REACT_APP_MOCK=true npm start",
        "server:install": "cd server && npm ci",
        "server:build": "tsc -b server/tsconfig.json",
        "server:start": "node server/build/server.js",
        "prepare": "husky install"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5",
        "arrowParens": "always",
        "printWidth": 100
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
